    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^4.1.1",
    "prettier": "^1.15.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^16.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.0",
    "spectron": "^3.4.1",
